symmetry,nosym
geometry={
1

Li  0.0 0.0 0.0
}

spin=1

basis={
default,aug-cc-pwCVTZ
set,ri
default,aug-cc-pwCVQZ/mp2fit
set,oep
default,aug-cc-pVDZ/mp2fit
}


df-uks,pbe,maxit=0,df_basis=aug-cc-pwCVQZ/mp2fit ! KS PBE
{cfit,basis_coul=aug-cc-pwCVQZ/mp2fit,basis_exch=aug-cc-pwCVQZ/mp2fit}

acfd;uscrpa,thr_fai_oep=5d-2
e(1)=energy
prog(1)='Li with dfit'

acfd;uscrpa,thr_fai_oep=5d-2,dfit=0
e(2)=energy
prog(2)='Li w/o dfit'

acfd;uscrpa,spin_sym=1,thr_fai_oep=5d-2
e(3)=energy
prog(3)='Li with dfit spin-symmetrization'

acfd;uscrpa,spin_sym=1,thr_fai_oep=5d-2,dfit=0
e(4)=energy
prog(4)='Li w/o dfit spin-symmetrizazion'

eold = [-7.526036305434, -7.526036270242, -7.525358571975, -7.525358566883]

de=e-eold(1:ii)

text,Results for job li_acfd_uscrpa.test
if(max(abs(de)).lt.1.d-6) then
  table,prog,e,eold,de
  save,test.log
  title,No errors in li_acfd_uscrpa.test
else
  table,prog,e,eold,de
  save,test.log
  title,ERRORS DETECTED in li_acfd_uscrpa.test, de=$de
endif
